maiorNum :: Int -> Int -> String
maiorNum x y 
         | (x>y)  = show x ++ " > " ++ show y
         | (x<y)  = show x ++ " < " ++ show y
         | (x==y) = show x ++ " = " ++ show y


mensao:: Integer -> String
mensao nota
        | (nota >= 0 && nota < 3)   = "SR"
        | (nota >= 3 && nota < 5)   = "MI"
        | (nota >= 5 && nota < 7)   = "MM"
        | (nota >= 7 && nota < 9)   = "MS"
        | (nota >= 9 && nota <= 10) = "SS"
        | otherwise                 = "Nota Invalida"
